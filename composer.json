{
    "name": "autowise/webhook-example-php",
    "description": "Example of decoding JWE webhooks in PHP",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "web-token/jwt-library": "^4.0",
        "spomky-labs/aes-key-wrap": "^7.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "suggests": {
        "ext-sodium": "*",
        "ext-bcmath": "*",
        "ext-openssl": "*"
    },
    "scripts": {
        "execute": "php src/decode.php"
    }
}
